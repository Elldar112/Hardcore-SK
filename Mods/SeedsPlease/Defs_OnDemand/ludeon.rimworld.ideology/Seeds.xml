  <SeedsPlease.SeedDef ParentName="SeedBase">
    <defName>Seed_Fibercorn</defName>
    <label>fibercorn seeds</label>
    <sources>
      <li>Plant_Fibercorn</li>
    </sources>
    <graphicData><texPath>Things/Item/Seeds/Fibercorn</texPath></graphicData>
    <seed>
      <harvestFactor>1.0</harvestFactor>
      <seedFactor>1.5</seedFactor>
      <baseChance>0.95</baseChance>
      <extraChance>0.45</extraChance>
    </seed>
  </SeedsPlease.SeedDef>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Fibercorn</defName>
    <label>find fibercorn seeds</label>
    <description>find seeds from grass</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Hay</li>
          </thingDefs>
        </filter>
        <count>8</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Hay</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Fibercorn>4</Seed_Fibercorn>
    </products>
  </RecipeDef>
  
