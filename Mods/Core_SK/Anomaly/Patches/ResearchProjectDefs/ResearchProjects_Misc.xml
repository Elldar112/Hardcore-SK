<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationRemove">
		<xpath>Defs/ResearchProjectDef[defName="SecurityDoor"]</xpath>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ScenarioDef/scenario/parts/li[@Class="ScenPart_StartingResearch" and project="SecurityDoor"]/project</xpath>
		<value>
			<project>Fortification_C2</project>
		</value>
	</Operation>

	<Operation Class="PatchOperationAttributeSet">
		<xpath>Defs/ResearchProjectDef[defName="TurretPack"]</xpath>
		<attribute>ParentName</attribute>
		<value>SK.HitechBaseDummy</value>
	</Operation>

	<Operation Class="SK.PatchOperationReplaceExtended">
		<xpath>Defs/ResearchProjectDef[defName="TurretPack"]</xpath>
		<value>
			<tab>Apparel_SK</tab>
			<researchViewX>19.00</researchViewX>
			<researchViewY>43.00</researchViewY>
		</value>
	</Operation>
	
</Patch>