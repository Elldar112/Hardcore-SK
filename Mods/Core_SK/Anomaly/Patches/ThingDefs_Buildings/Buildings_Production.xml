<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationRemove">
		<xpath>Defs/ThingDef[defName="BioferriteShaper" or defName="SerumCentrifuge"]/constructEffect</xpath>
	</Operation>

	<Operation Class="SK.PatchOperationReplaceExtended">
		<xpath>Defs/ThingDef[defName="BioferriteShaper"]</xpath>
		<value>
			<stuffCategories>
				<li>Metallic</li>
			</stuffCategories>
			<costStuffCount>50</costStuffCount>
			<costList>
				<ComponentIndustrial>2</ComponentIndustrial>
				<Bioferrite>40</Bioferrite>
			</costList>
		</value>
	</Operation>


	<Operation Class="SK.PatchOperationReplaceExtended">
		<xpath>Defs/ThingDef[defName="SerumCentrifuge"]</xpath>
		<value>
			<stuffCategories>
				<li>Metallic</li>
			</stuffCategories>
			<costStuffCount>100</costStuffCount>
			<costList>
				<Shard>1</Shard>
				<ComponentIndustrial>2</ComponentIndustrial>
				<Bioferrite>80</Bioferrite>
				<Steel>100</Steel>
			</costList>
		</value>
	</Operation>

	<Operation Class="SK.PatchOperationReplaceExtended">
		<xpath>Defs/ThingDef[defName="BioferriteHarvester"]</xpath>
		<value>
			<stuffCategories>
				<li>Metallic</li>
			</stuffCategories>
			<costStuffCount>50</costStuffCount>
			<costList>
				<ComponentIndustrial>1</ComponentIndustrial>
			</costList>
		</value>
	</Operation>


</Patch>